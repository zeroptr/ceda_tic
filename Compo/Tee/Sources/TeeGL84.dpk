package TeeGL84;

{$R *.RES}
{$ALIGN ON}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{ DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{ LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $00400000}
{$DESCRIPTION 'TeeChart Pro 8 for OpenGL'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  VCL40,
  Tee84,
  TeeUI84;

contains
  OpenGL2 in 'OpenGL2.pas',
  TeeGeometry in 'TeeGeometry.pas',
  TeeGLCanvas in 'TeeGLCanvas.pas',
  TeeOpenGL in 'TeeOpenGL.pas',
  TeeGLEditor in 'TeeGLEditor.pas' {FormTeeGLEditor};

end.
